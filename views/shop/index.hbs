{{# each products }}
<div class="row">
    {{# each this }}
    <div class="col-sm-6 col-md-4">
        <div class="img-thumbnail">
            <img src="{{ this.imagePath }}" alt="..." class="img-responsive">
            <div class="figure-caption">
                <h3>{{ this.title }}</h3>
                <p class="description">{{ this.description }}</p>
                <div class="clearfix">
                    <div class="price float-left">${{ this.price }}</div>
                    <a href="#" class="btn btn-success active float-right" role="button">Add to Shopping Cart</a>
                </div>
            </div>
        </div>
    </div>
    {{/each}}
</div>
{{/each}}